{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "plugin:prettier/recommended",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier": [
            "error",
            {"endOfLine": "auto"}],
        "no-console": "off",
        "quotes": [
            "error",
            "double"
        ],
        "linebreak-style": ["error", "windows"]
    }
}
